<aura:component implements="flexipage:availableForAllPageTypes" controller="afTimelineController">
    <aura:attribute name="chartTitle" type="String"/>
    <aura:attribute name="trainers" type="List[]"/>
    <aura:attribute name="data" type="List"/>
    <ltng:require scripts="{!join(',',$Resource.highchartsJS, $Resource.xrange, $Resource.exporting)}" afterScriptsLoaded="{!c.doInit}"/>
	<aura:registerEvent name="CreateJSON" type="c:CreateListOfStringJSON"/>
    <aura:handler name="CreateJSON" event="c:CreateListOfStringJSON" action="{!c.createJSON}"/>

    <aura:handler event="c:UpdateChartEvent" action="{!c.updateChart}"/>    
    
    <div aura:id="container"></div>
    <aura:iteration items="{!v.yAxisNames}" var="name">
        {!name}
    </aura:iteration>
    Hello{!v.yAxisNames}
    
    
</aura:component>